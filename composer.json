{
    "name": "laplace-demon-ai/laravel-mcp-log",
    "description": "MCP tool for Laravel log analysing with AI.",
    "keywords": [
        "Laplace Demon AI",
        "laravel",
        "laravel-mcp-log",
        "php 8"
    ],
    "type": "package",
    "homepage": "https://github.com/laplace-demon-ai/laravel-mcp-log",
    "license": "MIT",
    "authors": [
        {
            "name": "Moe Mizrak"
        }
    ],
    "require": {
        "php": "^8.4",
        "laravel/mcp": "^0.3.0",
        "moe-mizrak/laravel-log-reader": "^0.0.6"
    },
    "require-dev": {
        "orchestra/testbench": "^9.6.1",
        "phpunit/phpunit": "^11.4",
        "laravel/pint": "^1.24"
    },
    "autoload": {
        "psr-4": {
          "LaplaceDemonAI\\LaravelMcpLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "LaplaceDemonAI\\LaravelMcpLog\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
          "providers": [
            "LaplaceDemonAI\\LaravelMcpLog\\LaravelMcpLogServiceProvider"
          ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
